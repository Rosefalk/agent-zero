{
	"compilerOptions": {
		"noImplicitAny": true,
		"removeComments": true,
		"sourceMap": true,
		"target": "ESNext",
		"outDir": "dist",
		"module": "ESNext",
		"moduleResolution": "NodeNext",
		"allowImportingTsExtensions": true,
		"forceConsistentCasingInFileNames": true,
		"noEmit": true,
	},
	"include": [
		"./src/**/*",
	],
}